#!/usr/bin/env node
/* -*- mode: javascript -*-

  This file is part of JSlich, a JavaScript license checker.
  Copyright 2013, Kuno Woudt <kuno@frob.nl>.
  License: copyleft-next 0.3.0

*/

var request = require('request');
var jslich = require('../');

var t = 'http://localhost/jslich/public_html/free-inline-free-external/external.js';

request(t, function (error, response, body) {
    if (!error && response.statusCode == 200) {
        jslich.analyse.js (body);
    }
});

